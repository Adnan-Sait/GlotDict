{

  "description": "Add custom dictonaries on translate.wordpress.org",
  "manifest_version": 2,
  "name": "GlotDict",
  "version": "1.0.9",
  "homepage_url": "https://github.com/Mte90/GlotDict",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },

  "applications": {
    "gecko": {
      "id": "glotdict@mte90.net",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://translate.wordpress.org/*"],
      "js": ["init.js"]
    }
  ],
  "web_accessible_resources": [
      "glotdict.js",
      "keymaster.js",
      "dictionaries/it_IT.json",
      "dictionaries/fr_FR.json",
      "dictionaries/nl_NL.json",
      "dictionaries/de_DE.json",
      "dictionaries/bg_BG.json",
      "dictionaries/es_ES.json",
      "dictionaries/ja.json",
      "dictionaries/lt_LT.json",
      "dictionaries/sk_SK.json",
      "dictionaries/sv_SE.json",
      "dictionaries/fi.json",
      "dictionaries/hi_IN.json",
      "dictionaries/he_IL.json",
      "dictionaries/th.json",
      "dictionaries/en_AU.json",
      "dictionaries/ro_RO.json",
      "dictionaries/en_CA.json",
      "dictionaries/tr_TR.json",
      "dictionaries/ast.json"
  ]

}
